-- Miranda Hub Brainrot - Español
-- Inspirado en Miranda Hub TikTok | senkooooo666

if game.CoreGui:FindFirstChild("MirandaHub") then
    game.CoreGui.MirandaHub:Destroy()
end

local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "MirandaHub"

local frame = Instance.new("Frame", gui)
frame.Name = "MainFrame"
frame.BackgroundColor3 = Color3.fromRGB(30,30,30)
frame.BorderSizePixel = 0
frame.Position = UDim2.new(0.2,0,0.2,0)
frame.Size = UDim2.new(0,400,0,370)
frame.Active = true
frame.Draggable = true

local title = Instance.new("TextLabel", frame)
title.Text = "Miranda Hub - Brainrot"
title.Size = UDim2.new(0,400,0,36)
title.BackgroundColor3 = Color3.fromRGB(40,40,40)
title.Font = Enum.Font.GothamBold
title.TextColor3 = Color3.fromRGB(255,255,255)
title.TextSize = 22
title.BorderSizePixel = 0

-- Pestañas
local tabs = {"Main", "Jugador", "Trolleos", "Extras"}
local tabButtons = {}
local tabFrames = {}

for i, tab in ipairs(tabs) do
    local btn = Instance.new("TextButton", frame)
    btn.Name = tab.."Tab"
    btn.Text = tab
    btn.Position = UDim2.new(0, 10 + (i-1)*95, 0, 40)
    btn.Size = UDim2.new(0, 90, 0, 30)
    btn.BackgroundColor3 = Color3.fromRGB(60,60,60)
    btn.Font = Enum.Font.Gotham
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.TextSize = 18
    tabButtons[tab] = btn
    local tabFrame = Instance.new("Frame", frame)
    tabFrame.Position = UDim2.new(0,10,0,80)
    tabFrame.Size = UDim2.new(0,380,0,270)
    tabFrame.BackgroundTransparency = 1
    tabFrame.Visible = (i == 1)
    tabFrames[tab] = tabFrame
    btn.MouseButton1Click:Connect(function()
        for _, f in pairs(tabFrames) do f.Visible = false end
        tabFrame.Visible = true
    end)
end

-- Selección jugador objetivo
local playerLabel = Instance.new("TextLabel", tabFrames["Main"])
playerLabel.Text = "Jugador objetivo:"
playerLabel.Position = UDim2.new(0,10,0,10)
playerLabel.Size = UDim2.new(0,150,0,24)
playerLabel.BackgroundTransparency = 1
playerLabel.Font = Enum.Font.Gotham
playerLabel.TextColor3 = Color3.fromRGB(220,220,220)
playerLabel.TextSize = 16

local playerBox = Instance.new("TextBox", tabFrames["Main"])
playerBox.PlaceholderText = "Nombre exacto"
playerBox.Position = UDim2.new(0,160,0,10)
playerBox.Size = UDim2.new(0,200,0,24)
playerBox.BackgroundColor3 = Color3.fromRGB(60,60,60)
playerBox.Font = Enum.Font.Gotham
playerBox.TextColor3 = Color3.fromRGB(255,255,255)
playerBox.TextSize = 16

-- Función para encontrar jugador objetivo
local function getTarget()
    local name = playerBox.Text
    return game.Players:FindFirstChild(name)
end

-- Main Tab: Tp, Bring, Kill, Fling
local actions = {
    {"Teletransportar (Tp)", function()
        local plr = game.Players.LocalPlayer
        local target = getTarget()
        if target and target.Character and plr.Character then
            plr.Character:MoveTo(target.Character.HumanoidRootPart.Position + Vector3.new(2,0,2))
        end
    end},
    {"Traer jugador (Bring)", function()
        local plr = game.Players.LocalPlayer
        local target = getTarget()
        if target and target.Character and plr.Character then
            target.Character:MoveTo(plr.Character.HumanoidRootPart.Position + Vector3.new(2,0,2))
        end
    end},
    {"Fling jugador", function()
        local target = getTarget()
        if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
            local hrp = target.Character.HumanoidRootPart
            hrp.Velocity = Vector3.new(0, 1000, 0)
        end
    end},
    {"Kill jugador", function()
        local target = getTarget()
        if target and target.Character and target.Character:FindFirstChild("Humanoid") then
            target.Character.Humanoid.Health = 0
        end
    end},
}
for i, act in ipairs(actions) do
    local btn = Instance.new("TextButton", tabFrames["Main"])
    btn.Text = act[1]
    btn.Position = UDim2.new(0,10,0,40+(i-1)*38)
    btn.Size = UDim2.new(0,360,0,30)
    btn.BackgroundColor3 = Color3.fromRGB(20,120,180)
    btn.Font = Enum.Font.Gotham
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.TextSize = 16
    btn.MouseButton1Click:Connect(act[2])
end

-- Jugador Tab: Fly, Noclip, Godmode, WalkSpeed, JumpPower
local flyEnabled, noclipEnabled, godmodeEnabled = false, false, false
local lp = game.Players.LocalPlayer

local function toggleFly()
    flyEnabled = not flyEnabled
    local char = lp.Character
    if flyEnabled and char then
        local bp = Instance.new("BodyPosition", char.HumanoidRootPart)
        bp.Name = "MirandaFly"
        bp.MaxForce = Vector3.new(400000,400000,400000)
        bp.Position = char.HumanoidRootPart.Position
        game:GetService("RunService").RenderStepped:Connect(function()
            if flyEnabled and char and char:FindFirstChild("HumanoidRootPart") and bp then
                bp.Position = char.HumanoidRootPart.Position + Vector3.new(0,3,0)
            end
        end)
    else
        if char and char:FindFirstChild("HumanoidRootPart"):FindFirstChild("MirandaFly") then
            char.HumanoidRootPart.MirandaFly:Destroy()
        end
    end
end

local function toggleNoclip()
    noclipEnabled = not noclipEnabled
end

game:GetService("RunService").Stepped:Connect(function()
    if noclipEnabled and lp.Character then
        for _,v in pairs(lp.Character:GetChildren()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end
end)

local function toggleGodmode()
    godmodeEnabled = not godmodeEnabled
    if godmodeEnabled and lp.Character and lp.Character:FindFirstChild("Humanoid") then
        lp.Character.Humanoid.MaxHealth = math.huge
        lp.Character.Humanoid.Health = math.huge
    elseif lp.Character and lp.Character:FindFirstChild("Humanoid") then
        lp.Character.Humanoid.MaxHealth = 100
    end
end

local btnFly = Instance.new("TextButton", tabFrames["Jugador"])
btnFly.Text = "Fly"
btnFly.Position = UDim2.new(0,10,0,10)
btnFly.Size = UDim2.new(0,170,0,30)
