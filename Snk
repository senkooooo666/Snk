-- Trolleos Tab: Chat Spam, Sound Spam, Invisible, Reset
local trollActions = {
    {"Chat Spam", function()
        for i=1,10 do
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("¡Miranda Hub Brainrot!", "All")
            wait(0.2)
        end
    end},
    {"Sound Spam", function()
        local char = lp.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            for i=1,5 do
                local s = Instance.new("Sound", char.HumanoidRootPart)
                s.SoundId = "rbxassetid://142376088"
                s.Volume = 5
                s:Play()
                game:GetService("Debris"):AddItem(s, 2)
                wait(0.3)
            end
        end
    end},
    {"Invisible", function()
        local char = lp.Character
        if char then
            for _,v in pairs(char:GetChildren()) do
                if v:IsA("BasePart") then
                    v.Transparency = 1
                end
            end
        end
    end},
    {"Reset", function()
        if lp.Character and lp.Character:FindFirstChild("Humanoid") then
            lp.Character.Humanoid.Health = 0
        end
    end},
}

for i, act in ipairs(trollActions) do
    local btn = Instance.new("TextButton", tabFrames["Trolleos"])
    btn.Text = act[1]
    btn.Position = UDim2.new(0,10,0,10+(i-1)*38)
    btn.Size = UDim2.new(0,360,0,30)
    btn.BackgroundColor3 = Color3.fromRGB(180,40,80)
    btn.Font = Enum.Font.Gotham
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.TextSize = 16
    btn.MouseButton1Click:Connect(act[2])
end

